#Лошадь, годы жизни, породы
#Horse, years of life, breeds
SELECT
  ?horse
  ?horseLabel
  (YEAR(?birthDate) AS ?birthYear)
  (YEAR(?deathDate) AS ?deathYear)
  ?genderLabel
WHERE {
  ?horse wdt:P31 wd:Q726.  # Особа — лошадь
  ?horse wdt:P569 ?birthDate.  # Дата рождения
  OPTIONAL { ?horse wdt:P570 ?deathDate. }  # Дата смерти
  OPTIONAL { ?horse wdt:P21 ?gender. }  # Пол
  SERVICE wikibase:label { bd:serviceParam wikibase:language "ru,en". }
}
